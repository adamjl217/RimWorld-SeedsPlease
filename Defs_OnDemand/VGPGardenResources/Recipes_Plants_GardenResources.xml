<Defs>

  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeedSteelLeaf</defName>
    <label>extract steelleaf seeds</label>
    <description>Extract seeds from SteelLeaf.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawSteelLeaf</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawSteelLeaf</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <SeedSteelLeaf>3</SeedSteelLeaf>
    </products>
    <researchPrerequisite>resourceleafs</researchPrerequisite>
  </RecipeDef>

